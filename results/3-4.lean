  ] "utilize_mod_cycle"
  have h11 := Claim (List.Mem (3 ^ x % 109) [18, 33, 83, 68]) [
    {prop := List.Mem (229 ^ y % 109) [22, 37, 87, 72], proof := h10},
    {prop := 3 ^ x + 4 = 229 ^ y, proof := h3},
  ] "compute_mod_sub"
  have h12 := Claim False [
    {prop := x % 1 = 0, proof := h4},
    {prop := x >= 1, proof := h1},
    {prop := List.Mem (3 ^ x % 109) [18, 33, 83, 68], proof := h11},
