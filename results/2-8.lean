For positive integers x, y satisfying 2 ^ x + 8 = 120 ^ y,
if x >= 4 and y >= 4,
8 = 0 (mod 16), which is impossible.
Therefore, x < 4 or y < 4.
Further examination shows that 2 ^ x + 8 = 120 ^ y is impossible.
-/
theorem diophantine1_2_8_120 (x : Nat) (y : Nat) (h1 : x >= 1) (h2 : y >= 1) (h3 : 2 ^ x + 8 = 120 ^ y) :
  False
  := by
  have h4 : x % 1 = 0 := by omega
  have h5 : y % 1 = 0 := by omega
  by_cases h6 : And (x >= 4) (y >= 4)
  have h7 := Claim (2 ^ x % 16 = 0) [
    {prop := x % 1 = 0, proof := h4},
    {prop := x >= 4, proof := h6.left},
  ] "pow_mod_eq_zero"
  have h8 := Claim (120 ^ y % 16 = 0) [
    {prop := y % 1 = 0, proof := h5},
    {prop := y >= 4, proof := h6.right},
  ] "pow_mod_eq_zero"
  omega
  have h7 : Or (x <= 3) (y <= 3) := by omega
  have h8 := Claim False [
    {prop :=  x % 1 = 0, proof := h4},
    {prop :=  x >= 1, proof := h1},
    {prop :=  y % 1 = 0, proof := h5},
    {prop :=  y >= 1, proof := h2},
    {prop := 2 ^ x + 8 = 120 ^ y, proof := h3},
    {prop := Or (x <= 3) (y <= 3), proof := h7},
  ] "diophantine1_enumeration"
  exact h8

